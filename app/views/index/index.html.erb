    <div class='container-fluid'>
      <p id="notice"><%= notice %></p>
      <div class='row py-3 px-3'>
        <div class='col-12 col-sm-8'>
          <h3 class=''>Contacts</h3>
          <table id='contact-table' class="table table-hover">
            <thead  class="thead-light">
              <tr>
                <th>Name</th>
                <th>Phone</th>
                <th>Email</th>
                <th>Interest</th>
                <th colspan="3"></th>
              </tr>
            </thead>

            <tbody>
              <% @contacts.each do |contact| %>
                <tr>
                  <td><%= contact.Name %></td>
                  <td><%= contact.Phone %></td>
                  <td><%= contact.Email %></td>
                  <td><%= contact.Interest %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
          <br>
        </div>
      </div>
    </div>